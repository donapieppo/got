<%= render partial: 'what' %>  

<h1>Strutture Unibo</h1>

<dl class="organizations_list">
  <% @organizations.each do |organization| %>  
    <dt><%= link_to organization.name, organization_path(organization) %> <small><%= organization.description %></small></dt>
    <dd><%= t :admins %>: 
      <em>
        <%= raw organization.permissions.map {|p| mail_to p.user.upn, p.user.to_s}.join(', '.html_safe) %>  
      </em>
    </dd>
  <% end %>
</dl>
