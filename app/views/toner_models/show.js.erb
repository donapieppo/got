<% if @modal %>  
  $('#main-modal').modal('show');
  $('.modal-title').html("<%= j(@toner_model.to_s) %>");
  $('.modal-body').html("<%= j(render(@toner_model)) %>");
  $('.modal-footer').html("<%= j(
    link_to('edit', [:edit, @toner_model], class: :button) +
    link_to('add to ' + @current_organization.name, new_toner_model_toner_path(@toner_model), class: :button)
  ) %>");
<% end %>

