$(function(){
  // hide all models and show only vendor models already checked
  $("#models>div").hide();
  $("input:checked").parents(".vendor_group").show();
  // on vendor click show its models
  $(".vendor a").click(function(evt) {
    evt.preventDefault();
    vendor_id = evt.target.id;
    $("input:checkbox").each(function(index) {
      $("#vendor" + vendor_id).show();
    });
  });
});


