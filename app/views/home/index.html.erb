<%= render Organization::AvailableTonerComponent.new(organization: current_organization) %>

<% if policy(current_organization).edit? %>  
  <div class="organization-owning my-5">
    <div class="toners">
      <h2><%= dm_icon :tint %> <%= t :toners %> presenti in <%= current_organization.name %></h2> 
      <%= render 'toners', organization: current_organization %>  
    </div>

    <div class="printers">
      <h2><%= dm_icon :print %> <%= t :printers %> registrate in <%= current_organization.name %></h2>
      <%= render 'printers', organization: current_organization %>  
    </div>
  </div>
<% end %>

<hr/>

<div class='my-5'>
  <%= render 'admins', organization: current_organization %>  
</div>
