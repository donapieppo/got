<h1><%= @organization.name %> <small><%= @organization.description %></small></h1>

<div>
  <%= render 'admins', organization: @organization %>  
</div>

<div>
  <h2><%= icon :gift %> Toner disponibili per le tue stampanti</h2>
  <%= render 'available_toners', organization: @organization %>  
</div>

<div id="organization_owning">
  <div class="toners">
    <h2><%= icon :tint %> Toner <small>in <%= @organization.name %></small></h2>
    <%= render 'toners', organization: @organization %>  
  </div>

  <div class="printers">
    <h2><%= icon :print %> Stampanti <small>in <%= @organization.name %></small></h2>
    <%= render 'printers', organization: @organization %>  
  </div>
</div>

