<div class="row">
<div class="col-md-6">
  <%= render 'form' %>  
</div>
<div class="col-md-6">
  <%= dm_card title: t(:admins) do %>  
    <ul>
      <% @organization.admins.each do |admin| %>  
        <li><%= admin %> <span class="float-right"><%= link_to_delete admin_path(admin) %></span></li>
      <% end %>
    </ul>
    <%= link_to_new 'Nuovo Amministratore', new_organization_admin_path(@organization) %>  
  <% end %>
</div>
</div>



