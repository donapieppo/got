<strong><%= t :admins %></strong>: 
<% organization.admins.each do |admin| %>  
  <%= mail_to admin.user.email, icon(:envelope) + admin.user.cn %> 
  <%= link_to_delete(admin_path(admin)) if modifiable %>  
<% end %>

<% if modifiable %>  
  - <%= link_to 'nuovo amministratore', new_organization_admin_path(organization) %>  
<% end %>


